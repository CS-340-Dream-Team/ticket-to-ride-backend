    "@babel/preset-typescript": "^7.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "precise-commits": "^1.0.2",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.1",
    "typescript": "^3.0.1"
  },
